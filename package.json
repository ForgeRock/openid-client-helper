{
  "name": "openid-client-helper",
  "version": "0.1.0",
  "description": "An extension to [openid-client](https://www.npmjs.com/package/openid-client) for automated maintenance and transparent application of [OAuth 2.0](https://tools.ietf.org/html/rfc6749) access tokens.",
  "main": "lib/index",
  "scripts": {
    "test": "echo \"Error: no test specified\"",
    "lint": "eslint ./lib",
    "docs": "jsdoc2md ./lib/* > ./docs/README.md",
    "prepare": "npm run lint && npm run docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ForgeRock/openid-client-helper.git"
  },
  "keywords": [
    "oauth",
    "oauth2",
    "openid",
    "oidc",
    "relying party",
    "openid-client",
    "privilege restriction",
    "audience restricted",
    "scope restricted",
    "access token",
    "resource indicators"
  ],
  "author": "Konstantin Lapine <Konstantin.Lapine@forgerock.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ForgeRock/openid-client-helper/issues"
  },
  "homepage": "https://github.com/ForgeRock/openid-client-helper#readme",
  "dependencies": {
    "node-fetch": "^2.6.0",
    "openid-client": "^3.14.0"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "jsdoc-to-markdown": "^5.0.3"
  },
  "engines": {
    "node": "^10.13.0 || >=12.0.0"
  },
  "type": "commonjs"
}
